{
    "name": "@react/cross-platform",
    "author": "femijoshuaalabi",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "workspaces": {
        "packages": [
            "ApplicationBasement/*",
            "ApplicationBasement/WebApplication/_CrossBasement/*"
        ]
    },
    "scripts": {
        "app:android": "yarn workspace @react/native-app run android",
        "app:release_android": "yarn workspace @react/native-app run release_android",
        "app:ios": "yarn workspace @react/native-app run ios",
        "app:start": "yarn workspace @react/native-app run start",
        "app:link": "yarn workspace @react/native-app run link",
        "expo:start": "yarn workspace @react/web-app run expo",
        "expo:build": "yarn workspace @react/web-app run expo:build",
        "web:optimize": "yarn workspace @react/web-app run optimize",
        "format": "yarn workspaces run format",
        "lint": "yarn workspaces run lint",
        "patch-package": "patch-package --patch-dir patches",
        "patch-update": "patch-package react-scripts"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/preset-react": "7.12.13",
        "@babel/runtime": "^7.12.5",
        "@types/extra-watch-webpack-plugin": "1.0.1",
        "@types/jest": "26.0.7",
        "@types/node": "14.0.27",
        "@types/react": "16.9.43",
        "@types/react-native": "0.63.2",
        "@types/webpack": "4.41.21",
        "@typescript-eslint/eslint-plugin": "3.7.1",
        "@typescript-eslint/parser": "3.7.1",
        "eslint": "^7.18.0",
        "eslint-config-prettier": "6.11.0",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-import": "2.22.0",
        "eslint-plugin-jest": "23.19.0",
        "eslint-plugin-react": "7.20.5",
        "eslint-plugin-react-hooks": "4.0.8",
        "eslint-plugin-react-native": "3.8.1",
        "expo-optimize": "0.1.86",
        "extra-watch-webpack-plugin": "1.0.3",
        "jest": "^26.6.3",
        "jetifier": "^1.6.6",
        "patch-package": "6.2.2",
        "postinstall-postinstall": "2.1.0",
        "prettier": "2.0.5",
        "sass-loader": "7.3.1",
        "sharp-cli": "1.15.0",
        "typescript": "3.9.7",
        "memoize-one": "5.2.1"
    },
    "dependencies": {
        "emoji-datasource-apple": "^6.0.1",
        "emoji-datasource-facebook": "^6.0.1",
        "emoji-datasource-google": "^6.0.1",
        "emoji-datasource-twitter": "^6.0.1"
    }
}
